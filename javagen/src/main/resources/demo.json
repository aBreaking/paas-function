{
  "name":"mytest",
  "desc":"调用远程服务然后再求个和",
  "tasks":[
    {
      "type":"com.sitech.paas.javagen.demo.util.ServiceCaller",
      "method":"callRest",
      "alias":"s1",
      "timeout":10000,
      "inputs":{
        "restUrl":"http://ip:port/my/rest/uri",
        "pin":"{\"mypin\":\"something\"}"
      },
      "inputsExtra":{
        "restUrl":"java.lang.String",
        "pin":"java.lang.String"
      },
      "returnType":"java.lang.String"
    },
    {
      "type":"1",
      "alias":"s2",
      "imports":"com.sitech.paas.javagen.demo.util.CommonUtil;com.alibaba.fastjson.JSONObject",
      "returnType":"JSONObject",
      "body":{
        "java":"String parse = ${s1}.substring(${s1}.indexOf(\":\")+1);JSONObject retJson= CommonUtil.parseJson(parse);return  retJson;"
      }
    },
    {
      "type":"com.sitech.paas.javagen.demo.util.CommonUtil",
      "method":"sum",
      "alias":"s3",
      "inputs":{
        "a":"${s2.first}",
        "b":"${s2.second}"
      },
      "inputsExtra":{
        "a":"java.lang.Integer",
        "b":"java.lang.Integer"
      },
      "returnType":"java.lang.Integer"
    }
  ],
  "output":{
    "age":"${s3}"
  }
}