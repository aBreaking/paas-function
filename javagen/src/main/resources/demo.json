{
  "name":"mytest",
  "desc":"调用远程服务然后再求个和",
  "tasks":[
    {
      "type":"com.sitech.paas.javagen.demo.util.ServiceCaller",
      "method":"callRest",
      "alias":"s1",
      "timeout":10000,
      "inputs":{
        "restUrl":"http://ip:port/my/rest/uri",
        "pin":"{\"mypin\":\"something\"}"
      },
      "inputsExtra":{
        "restUrl":"java.lang.String",
        "pin":"java.lang.String"
      },
      "returnType":"java.lang.String"
    },
    {
      "type":"1",
      "alias":"s2",
      "imports":"com.sitech.foo.CommonUtil;com.alibaba.fastjson.JsonObject",
      "returnType":"com.alibaba.fastjson.JsonObject",
      "body":{
        "java":"String[] split = s1.split(\":\");JsonObject = CommonUtil.parseJson(split[2]);return  JsonObject;"
      }
    },
    {
      "type":"0",
      "method":"sum",
      "alias":"s3",
      "inputs":{
        "a":"s2.first",
        "b":"s2.second"
      },
      "inputsExtra":{
        "a":"java.lang.Integer",
        "b":"java.lang.Integer"
      },
      "returnType":"java.lang.Integer"
    }
  ],
  "output":"$s3"
}